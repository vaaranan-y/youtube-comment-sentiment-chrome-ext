{
    "manifest_version" : 3,
    "name" : "Youtube Comment Sentiment Analyzer",
    "version" : "1.0",
    "description" : "Sentiment analyzer for youtube comments",
    "icons" : {
        "128" : "icon128.png",
        "48" : "icon48.png",
        "16" : "icon16.png"
    },
    "action" : {
        "default_icon" : "icon16.png",
        "default_popup" : "index.html"
    }, 
    "host_permissions": ["http://127.0.0.1:8000/"],
    "permissions": [
        "tabs"
    ]
}